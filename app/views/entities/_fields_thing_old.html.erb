
  <div class="field">
  <p>Name: <br />
    <%= f.text_field :name %>
  </p>
  </div>
  <div class="field">
  <p>LivePulseID: [optional]<br />
    <%= f.text_field :livePulseID %>
  </p>
  </div>
  <div class="field">
   <p>What space is this entity in?
   </br>
    <%= f.collection_select :uri, Entity.all_spaces, :downcase, :titleize %>
   </p>
  </div>
  <div class="field">
    <p>Description:
    <br />
    <%= f.text_area :desc %>
    </p>
  </div>
  <div class="field">
    <p>Ownership</br>
    <%= f.collection_select :ownership, Entity.ownershipTypes, :downcase, :titleize %>
    </p>
  </div>
  <div class="field">
    <p>Subtype<br />
    <%= f.collection_select :subtype, Entity.all_thing_types, :downcase, :titleize %>
    </p>
  </div>
